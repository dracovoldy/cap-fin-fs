{
    "name": "BTP-GLInvBalTax",
    "version": "1.0.0",
    "description": "A simple CAP project.",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap-cloud-sdk/http-client": "^2.7.1",
        "@sap/cds": ">=5",
        "@sap/cds-odata-v2-adapter-proxy": "^1.9.4",
        "express": "^4",
        "lodash.isequal": "^4.5.0"
    },
    "devDependencies": {
        "sqlite3": "^5.0.4"
    },
    "scripts": {
        "start": "cds run"
    },
    "engines": {
        "node": "^16.15"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "requires": {
            "API_GL_ACCOUNT_BALANCE": {
                "kind": "odata-v2",
                "model": "srv/external/API_GL_ACCOUNT_BALANCE",
                "credentials": {
                    "destination": "API_S4_HTTP_BASIC_APPDEVUSER",
                    "path": "/sap/opu/odata/sap/YCDS_GLACQ01_CDS/",
                    "forwardAuthToken": true
                }
            }
        },
        "auth": {
            "kind": "xsuaa"
        }
    },
    "sapux": [
        "app/glaccbalances"
    ]
}